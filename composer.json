{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "appstract/laravel-opcache": "^2.0",
        "davejamesmiller/laravel-breadcrumbs": "5.x",
        "fideloper/proxy": "^4.0",
        "fomvasss/laravel-dadata": "^2.0",
        "genealabs/laravel-model-caching": "*",
        "intervention/image": "^2.4",
        "laravel-doctrine/orm": "1.4.*",
        "laravel-frontend-presets/zurb-foundation": "^2.1",
        "laravel/framework": "5.8.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.8.0",
        "maatwebsite/excel": "^3.1",
        "unisharp/laravel-ckeditor": "^4.7"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "irazasyed/telegram-bot-sdk": "^3.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Helpers/HtmlOperations.php",
            "app/Helpers/StringOperation.php", 
            "app/Helpers/SessionOperation.php",
            "app/Helpers/PhotoOperations.php",
            "app/Helpers/PageInfo.php",
            "app/Helpers/RobotOperations.php",
            "app/Helpers/MessageOperations.php",
            "app/Helpers/PhoneOperations.php",
            "app/Helpers/Challenges.php",
            "app/Helpers/Filters.php",
            "app/Helpers/GeneralOperations.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
